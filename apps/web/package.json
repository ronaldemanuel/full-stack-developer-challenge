{
  "name": "@nx-ddd/web",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "name": "web",
    "targets": {
      "dev": {
        "continuous": true,
        "options": {
          "cwd": "apps/web",
          "command": "next dev --turbo"
        },
        "syncGenerators": [
          "@nx/js:typescript-sync"
        ],
        "executor": "nx:run-commands",
        "configurations": {},
        "parallelism": true
      }
    }
  },
  "dependencies": {
    "@nx-ddd/api-infrastructure": "workspace:*",
    "@nx-ddd/auth-domain": "workspace:*",
    "@nx-ddd/auth-infrastructure": "workspace:*",
    "@nx-ddd/jobs-events-infrastructure": "workspace:*",
    "@nx-ddd/shared-domain": "workspace:*",
    "@nx-ddd/shared-infrastructure": "workspace:*",
    "@nx-ddd/shared-presentation": "workspace:*",
    "@nx-ddd/tailwind": "workspace:*",
    "@nx-ddd/ui": "workspace:*",
    "@nx-ddd/ui-utils": "workspace:*",
    "@tanstack/react-query": "catalog:",
    "@trpc/client": "catalog:trpc",
    "@trpc/tanstack-react-query": "catalog:trpc",
    "next": "catalog:next",
    "next-safe-action": "catalog:next",
    "react": "catalog:react19",
    "react-dom": "catalog:react19",
    "react-qr-code": "catalog:",
    "trpc-ui": "catalog:trpc",
    "ua-parser-js": "catalog:"
  }
}
